{
  "compilerOptions": {
    "baseUrl": "",
    "declaration": false,
    "downlevelIteration": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "mapRoot": "./",
    "module": "commonjs",
    "moduleResolution": "node",
    "outDir": "../dist/out-tsc",
    "sourceMap": true,
    "target": "ES2015",
    "lib": ["es2015", "dom"],
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "typeRoots": ["../node_modules/@types"],
    "allowJs": true
  }
}
